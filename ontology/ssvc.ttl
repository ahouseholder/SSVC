@base <http://www.cert.org/ns/ssvc/> .

<http://www.cert.org/ns/ssvc> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.cert.org/ns/ssvc/hasValue
ssvc:hasValue rdf:type owl:ObjectProperty ;
              rdfs:domain ssvc:DecisionPoint ;
              rdfs:range ssvc:DecisionPointValue .


#################################################################
#    Classes
#################################################################

###  http://www.cert.org/ns/ssvc/ActiveSupplierEngagementValue
ssvc:ActiveSupplierEngagementValue rdf:type owl:Class ;
                                   rdfs:subClassOf ssvc:High ,
                                                   ssvc:SupplierEngagementValue .


###  http://www.cert.org/ns/ssvc/AmpliativePublicValueAddedValue
ssvc:AmpliativePublicValueAddedValue rdf:type owl:Class ;
                                     rdfs:subClassOf ssvc:High ,
                                                     ssvc:PublicValueAddedValue .


###  http://www.cert.org/ns/ssvc/Archetype
ssvc:Archetype rdf:type owl:Class ;
               rdfs:subClassOf ssvc:DecisionPointValue .


###  http://www.cert.org/ns/ssvc/Automatable
ssvc:Automatable rdf:type owl:Class ;
                 rdfs:subClassOf ssvc:DecisionPoint ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ssvc:hasValue ;
                                   owl:someValuesFrom ssvc:AutomatableValue
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ssvc:hasValue ;
                                   owl:allValuesFrom ssvc:AutomatableValue
